{
	"$schema": "http://json-schema.org/schema",
	"cli": "nx",
	"id": "SharedDDD",
	"title": "DDD - Shared kernel Generator",
	"type": "object",
	"properties": {
		"name": {
			"type": "string",
			"description": "",
			"default": "shared-kernel",
			"$default": {
				"$source": "argv",
				"index": 0
			},
			"x-prompt": "Project name"
		},
		"tags": {
			"type": "string",
			"description": "Add tags to the project (used for linting)",
			"alias": "t"
		}
	},
	"required": ["name"]
}
