{
	"$schema": "http://json-schema.org/schema",
	"name": "nextjs",
	"version": "0.0.1",
	"generators": {
		"nextjs": {
			"factory": "./src/generators/nextjs/generator",
			"schema": "./src/generators/nextjs/schema.json",
			"description": "Nextjs generator"
		},
		"react-lib": {
			"factory": "./src/generators/react-lib/generator",
			"schema": "./src/generators/react-lib/schema.json",
			"description": "React library generator"
		},
		"rc": {
			"factory": "./src/generators/react-components/generator",
			"schema": "./src/generators/react-components/schema.json",
			"description": "React component generator"
		},
		"nestjs": {
			"factory": "./src/generators/nestjs/generator",
			"schema": "./src/generators/nestjs/schema.json",
			"description": "NestJs generator"
		},
		"nestjs-bc": {
			"factory": "./src/generators/nestjs-bc/generator",
			"schema": "./src/generators/nestjs-bc/schema.json",
			"description": "NestJs bounded contexts generator"
		},
		"node-lib": {
			"factory": "./src/generators/node-lib/generator",
			"schema": "./src/generators/node-lib/schema.json",
			"description": "Node library generator"
		},
		"shared-kernel": {
			"factory": "./src/generators/shared-kernel/generator",
			"schema": "./src/generators/shared-kernel/schema.json",
			"description": "Shared kernel generator"
		},
		"docker": {
			"factory": "./src/generators/docker/generator",
			"schema": "./src/generators/docker/schema.json",
			"description": "Docker compose generator"
		}
	}
}
